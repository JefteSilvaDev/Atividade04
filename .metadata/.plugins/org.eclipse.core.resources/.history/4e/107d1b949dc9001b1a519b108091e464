import javax.swing.JOptionPane;

public class EntreXeY extends JOptionPane {


	public static void main(String[] args) {
		
		int num1 = Integer.parseInt(JOptionPane.showInputDialog("Digite 1 numero."));
		int num2 = Integer.parseInt(JOptionPane.showInputDialog("Digite 1 numero."));
		int num;
		int valor1 = num1;
		int valor2 = num2;
		
		if (num1 > num2) {
			for (; num2 < valor1; num1++){
			JOptionPane.showMessageDialog(null,"Os valores entre" +num2+ "e" +num1+ "são"+valor1);
			}	
		}	
		else if (num2 > num1) {	
			for (; num1 < valor2; num2--)
			JOptionPane.showMessageDialog(null,"Os valores entre" +num2+ "e" +num1+ "são"+valor2);
				}
			
		}
	}