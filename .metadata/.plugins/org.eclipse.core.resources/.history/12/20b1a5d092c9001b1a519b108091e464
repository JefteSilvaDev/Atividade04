import javax.swing.JOptionPane; 
public class EntreXeY {
	public static void main (String[] args) {
		int cont = 10;
                StringBuilder sb = new StringBuilder();
                for (int i = 0; i < 10; i++) {
                     for (int j = 0; j < cont; j++) {
                          sb.append("*"); 
                     }
                     sb.append("\n");
                     cont--;
                }
                JOptionPane.showMessageDialog(null, sb.toString());		
	}
}